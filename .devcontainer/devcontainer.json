{
  "name": "mini-pern/node",
  "dockerComposeFile": ["../docker-compose.yml"],
  "service": "node",
  "shutdownAction": "none",
  "customizations": {
    "vscode": {
      "extensions": [
        "esbenp.prettier-vscode",
        "Prisma.prisma",
        "dsznajder.es7-react-js-snippets"
      ],
      "settings": {
        "terminal.integrated.defaultProfile.linux": "zsh"
      }
    }
  },
  "postCreateCommand": "bash config.sh",
  "workspaceFolder": "/mini-pern",
  "remoteUser": "node"
}
