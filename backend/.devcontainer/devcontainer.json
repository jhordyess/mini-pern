{
  "name": "backend",
  "dockerComposeFile": ["../../docker-compose.yml"],
  "service": "backend",
  "shutdownAction": "none",
  "customizations": {
    "vscode": {
      "extensions": [
        "esbenp.prettier-vscode",
        "Prisma.prisma",
        "xabikos.JavaScriptSnippets"
      ],
      "settings": {
        "terminal.integrated.defaultProfile.linux": "zsh"
      }
    }
  },
  "postCreateCommand": "bash config.sh",
  "workspaceFolder": "/mini-pern/backend",
  "remoteUser": "node"
}
