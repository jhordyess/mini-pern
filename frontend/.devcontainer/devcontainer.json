{
  "name": "frontend",
  "dockerComposeFile": ["../../docker-compose.yml"],
  "service": "frontend",
  "shutdownAction": "none",
  "extensions": [
    "eg2.vscode-npm-script",
    "esbenp.prettier-vscode",
    "burkeholland.simple-react-snippets"
