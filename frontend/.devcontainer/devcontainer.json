{
  "name": "frontend",
  "dockerComposeFile": ["../../docker-compose.yml"],
  "service": "frontend",
  "shutdownAction": "none",
  "customizations": {
    "vscode": {
      "extensions": [
        "esbenp.prettier-vscode",
        "burkeholland.simple-react-snippets"
      ],
      "settings": {
        "terminal.integrated.defaultProfile.linux": "zsh"
      }
    }
  },
  "postCreateCommand": "bash config.sh",
  "workspaceFolder": "/mini-pern/frontend",
  "remoteUser": "node"
}
